[
    {
        "name": "httpd",
        "image": "library/httpd",
        "essential": true,
        "portMappings": [{
            "containerPort": 80
        }],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "fargate",
                "awslogs-region": "us-east-1",
                "awslogs-stream-prefix": "httpd"
            }
        },
        "memory": 512,
        "cpu": 256,
        "network_mode": "awsvpc"
    }
]