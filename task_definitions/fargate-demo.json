[
    {
        "name": "httpd",
        "image": "library/httpd",
        "essential": true,
        "portMappings": [{
            "containerPort": 80,
            "hostPort": 80
        }],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "fargate-demo",
                "awslogs-region": "us-east-1",
                "awslogs-stream-prefix": "httpd"
            }
        },
        "memory": 512,
        "cpu": 512
    }
]